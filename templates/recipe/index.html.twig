{% extends 'base.html.twig' %}

{% block title "Toute les recettes" %}
{% block body %}
    <ul>
        <h3>Recettes Marocaine</h3>
        {#  {{ dump(app) }}#}
        {% for recipe in recipes %}
        <li><a class ="nav-link" href = "{{ path('recipe.show', {id:recipe.id, slug: recipe.slug}) }}">{{ recipe.title }}</a></li>

        {% endfor %}
    </ul>
{% endblock %}